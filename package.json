{
  "name": "aquajarvis-vision",
  "version": "1.0.0",
  "description": "AquaJarvis Vision â€” Local Desktop Vision and Automation Tool",
  "main": "src/main.js",
  "homepage": "./",
  "author": {
    "name": "Waya Steurbaut",
    "email": "contact@wayashub.framer.ai"
  },
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "electron .",
    "dev": "electron . --dev",
    "build": "npm run build:renderer && npm run build:main",
    "build:renderer": "webpack --mode production",
    "build:main": "webpack --config webpack.main.config.js --mode production",
    "build:win": "electron-builder --win",
    "dist": "npm run build && electron-builder",
    "pack": "npm run build && electron-builder --dir",
    "postinstall": "electron-builder install-app-deps",
    "setup-dev": "node scripts/setup-dev.js",
    "setup-ollama": "node scripts/setup-ollama.js"
  },
  "dependencies": {
    "electron": "^28.0.0",
    "electron-store": "^8.1.0",
    "node-cron": "^3.0.3",
    "systeminformation": "^5.21.20",
    "robotjs": "^0.6.0",
    "sharp": "^0.33.1",
    "node-screenshot": "^1.0.2",
    "ws": "^8.16.0",
    "axios": "^1.6.5",
    "node-machine-id": "^1.1.12",
    "crypto-js": "^4.2.0",
    "sqlite3": "^5.1.6",
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "winston": "^3.11.0",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "electron-builder": "^24.9.1",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1",
    "html-webpack-plugin": "^5.6.0",
    "css-loader": "^6.8.1",
    "style-loader": "^3.3.3",
    "ts-loader": "^9.5.1",
    "typescript": "^5.3.3",
    "@types/node": "^20.10.5",
    "concurrently": "^8.2.2",
    "nodemon": "^3.0.2"
  },
  "build": {
    "appId": "com.wayasteurbaut.aquajarvis-vision",
    "productName": "AquaJarvis Vision",
    "directories": {
      "output": "dist"
    },
    "files": [
      "src/**/*",
      "assets/**/*",
      "node_modules/**/*",
      "package.json"
    ],
    "extraResources": [
      {
        "from": "tools",
        "to": "tools"
      },
      {
        "from": "scripts",
        "to": "scripts"
      }
    ],
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": ["x64", "arm64"]
        }
      ],
      "icon": "assets/icons/icon.ico",
      "requestedExecutionLevel": "asInvoker"
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true,
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true,
      "shortcutName": "AquaJarvis Vision"
    },
    "publish": {
      "provider": "github",
      "owner": "wayasteurbaut",
      "repo": "AquaJarvis-Vision"
    }
  },
  "keywords": [
    "electron",
    "automation",
    "vision",
    "ai",
    "local",
    "privacy",
    "desktop",
    "jarvis",
    "voice",
    "screen-capture"
  ]
}
